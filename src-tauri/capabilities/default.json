{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "core:path:default",
    "core:resources:default",
    "core:webview:default",

    "dialog:default",

    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        { "name": "binaries/qpdf", "sidecar": true }
      ]
    }
  ]
}
